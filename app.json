{
  "expo": {
    "name": "Accountrix",
    "slug": "accountrix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9FAFB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.accountrix.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app does not use the camera",
        "NSPhotoLibraryUsageDescription": "This app does not use the photo library",
        "NSMicrophoneUsageDescription": "This app does not use the microphone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F9FAFB"
      },
      "package": "com.accountrix.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-secure-store",
      "expo-file-system",
      "expo-sharing",
      "expo-document-picker",
      "expo-web-browser",
      "expo-auth-session",
      "expo-haptics",
      "expo-blur",
      "expo-linear-gradient",
      "expo-notifications",
      "expo-in-app-purchases"
    ],
    "scheme": "accountrix",
    "extra": {
      "eas": {
        "projectId": "accountrix-session-manager"
      }
    }
  },
  "name": "Accountrix",
  "displayName": "Accountrix - Account Session Manager"
}